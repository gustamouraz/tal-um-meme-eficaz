SELECT D3_NRBPIMS, D_E_L_E_T_, D3_ESTORNO, D3_LOTECTL, D3_DOC, D3_QUANT, * FROM SD3010 WITH(NOLOCK) 
    WHERE D3_NRBPIMS = '2470'
        AND D3_FILIAL = '060102'
        AND D3_EMISSAO = '20221107' 
        --AND D3_DOC = 'TS00410G3'
        ORDER BY R_E_C_N_O_

SELECT D_E_L_E_T_, D5_PRODUTO, D5_ESTORNO, D5_QUANT, D5_LOTECTL, * FROM SD5010 WITH(NOLOCK)
    WHERE D5_FILIAL = '060102'
        AND D5_DOC = 'TS0040102'
        AND D5_DATA = '20221107'
        AND D_E_L_E_T_ = ''
        AND D5_PRODUTO = '3120000024'
        ORDER BY R_E_C_N_O_

SELECT D3_NRBPIMS, D_E_L_E_T_, D3_ESTORNO, D3_LOTECTL, D3_DOC, D3_QUANT, * FROM SD3010 WITH(NOLOCK) 
    WHERE D3_COD = '3120000024'
        AND D3_LOCAL = '11' 
        AND D3_NRBPIMS = '2470'
        AND D3_EMISSAO = '20221107' 
        --AND D3_DOC = 'TS00410G3'
        ORDER BY R_E_C_N_O_

